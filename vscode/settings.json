{
  "ruby.useLanguageServer": false,
  "ruby.lint": {
    "rubocop": false
  },
  "ruby.codeCompletion": false,
  "ruby.intellisense": false,
  "ruby.format": false,
  "[ruby]": {
    "editor.defaultFormatter": "Shopify.ruby-lsp",
    "editor.formatOnSave": true,
    "editor.tabSize": 2,
    "editor.insertSpaces": true,
    "editor.formatOnType": true,
    "editor.semanticHighlighting.enabled": true,
    "files.trimTrailingWhitespace": true,
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "editor.rulers": [
      120
    ]
  },
  "byesig.fold": false,
  "byesig.enabled": true,
  "byesig.opacity": 0.5,
  "byesig.showIcon": false,
  "files.trimTrailingWhitespace": true,
  "files.insertFinalNewline": true,
  "rubyLsp.enabledFeatures": {
    "documentHighlights": true,
    "documentSymbols": true,
    "foldingRanges": true,
    "selectionRanges": true,
    "semanticHighlighting": true,
    "formatting": true,
    "diagnostics": true,
    "codeActions": true
  },
  "sorbet.enabled": true,
  "window.title": "${activeEditorMedium}${separator}${folderName}",
  "editor.lineNumbers": "on",
  "editor.rulers": [
    120
  ],
  "editor.fontSize": 14,
  "remote.SSH.connectTimeout": 150,
  "editor.bracketPairColorization.enabled": true,
  "editor.guides.bracketPairs":"active",
  "workbench.colorCustomizations": {
    "editorError.foreground": "#d85151",
    "minimap.errorHighlight": "#d85151",
    "editorGutter.deletedBackground": "#d85151",
    "editorMarkerNavigationError.background": "#d85151",
    "errorForeground": "#d85151"
  },
  "markdown-mermaid.lightModeTheme": "dark",
  "markdown-mermaid.languages": [

    "mermaid"
  ],
  "mermaid.vscode.light": "dark",
  "workbench.panel.defaultLocation": "right",
  "workbench.editor.wrapTabs": "true",
  "diffEditor.ignoreTrimWhitespace": false,
  "github.copilot.editor.enableAutoCompletions": true
}
